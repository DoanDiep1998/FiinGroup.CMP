@{
    Layout = "_Layout";
    var productCode = ViewData["ProductCode"] as string ?? string.Empty;
}

<h2>CMP Client</h2>
<p>Product code: @productCode</p>

<div style="width:100%;height:600px;border:1px solid #ccc;">
    <iframe src="@Url.Action("FrameContent", "CMPClient", new { productCode = productCode })" style="width:100%;height:100%;border:0;"></iframe>
</div>
